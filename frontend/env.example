# API URL for identity service
# Note: Port 6000 is blocked by Chrome (ERR_UNSAFE_PORT)
# Use ports like 3000, 3001, 8000, 8080, etc. instead
VITE_IDENTITY_API_URL=http://localhost:3000

# Backend API URL (NestJS)
VITE_BACKEND_API_URL=http://localhost:3000

# Kratos Public URL
VITE_KRATOS_URL=https://auth.foxia.vn

# Enable debug logging for Vite Proxy
VITE_DEBUG_PROXY=false


