{
  "name": "foxia-auth-backend",
  "version": "0.0.1",
  "description": "",
  "author": "Nguyễn Tiến Lộc",
  "private": true,
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "postbuild": "copyfiles -u 2 'share/protos/**/*' dist/share/protos",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:identity:dev": "yarn postbuild && nest start identity-service --watch",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./apps/like-68-backend/test/jest-e2e.json",
    "migration:identity:generate": "yarn build identity-service && yarn typeorm migration:generate ./apps/identity-service/src/migrations/migration -d dist/apps/identity-service/src/data-source.js",
    "migration:identity:run": "yarn build identity-service && yarn typeorm migration:run -d dist/apps/identity-service/src/data-source.js",
    "migration:identity:revert": "yarn build identity-service && yarn typeorm migration:revert -d dist/apps/identity-service/src/data-source.js"
  },
